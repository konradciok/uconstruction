{
  "name": "watercolor-artist-site",
  "version": "0.2.0",
  "description": "Premium portfolio and workshop booking platform with advanced product pages featuring gallery enhancements, sticky controls, and conversion optimization",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:full": "node scripts/start-dev.js",
    "dev:db": "npm run prisma:generate && npm run prisma:db:push && npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:reset": "prisma migrate reset --force",
    "prisma:db:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "db:backup": "node scripts/backup-sqlite.js",
    "db:migrate": "node scripts/simple-migrate.js",
    "db:switch": "./scripts/switch-db.sh",
    "postgres:setup": "./scripts/setup-postgres.sh",
    "postgres:start": "docker-compose up -d postgres",
    "postgres:stop": "docker-compose down",
    "migrate:full": "./scripts/full-migration.sh",
    "db:seed": "node scripts/seed-sample-data.js",
    "mcp:setup": "node scripts/setup-mcp.js",
    "mcp:test": "echo 'Testing MCP configuration...' && npx @stripe/mcp --tools=all && echo 'Stripe MCP working' && npx @modelcontextprotocol/server-postgres postgresql://test:test@localhost:5432/test && echo 'PostgreSQL MCP working'",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:startup": "./scripts/docker-startup.sh",
    "docker:clean": "docker-compose down -v && docker system prune -f"
  },
  "dependencies": {
    "@formspree/react": "^3.0.0",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.15.0",
    "@stripe/stripe-js": "^2.0.0",
    "clsx": "^2.1.1",
    "date-fns-tz": "^2.0.0",
    "framer-motion": "^12.23.12",
    "isomorphic-dompurify": "^2.11.0",
    "next": "^15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sqlite3": "^5.1.7",
    "stripe": "^14.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/pg": "^8.11.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "pg": "^8.12.0",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  }
}
