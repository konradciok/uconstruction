{
  "name": "watercolor-artist-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:full": "node scripts/start-dev.js",
    "dev:db": "npm run prisma:generate && npm run prisma:db:push && npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "shopify:verify": "node --env-file=.env --env-file=.env.local --env-file=env.local scripts/shopify-verify.js",
    "sync:backfill": "node --env-file=.env --env-file=.env.local --env-file=env.local scripts/shopify-backfill.js",
    "sync:delta": "node --env-file=.env --env-file=.env.local --env-file=env.local scripts/shopify-delta-sync.js",
    "webhooks:setup": "node --env-file=.env --env-file=.env.local --env-file=env.local scripts/setup-webhooks.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:db:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force"
  },
  "dependencies": {
    "@formspree/react": "^3.0.0",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.15.0",
    "@stripe/stripe-js": "^2.0.0",
    "date-fns-tz": "^2.0.0",
    "framer-motion": "^12.23.12",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "stripe": "^14.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "typescript": "^5.9.2"
  }
}
